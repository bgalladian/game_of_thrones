<h2> Edit: <%= @house.name %></h2>

<%= form_for  @house, url: "/houses/#{@house.id}" do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :motto %>
  <%= f.text_field :motto %>

  <%= f.label :img_url %>
  <%= f.text_field :img_url %>
<br>
<br>

<%= f.submit "Update"%>
<% end %>
